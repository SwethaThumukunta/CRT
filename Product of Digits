#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

    int main(){
        int n,product=1;
        scanf("%d",&n);
        while(n>0){
            int digit=0;
            digit=n%10;
            if(digit!=0){
                product=product*digit;
            }
            n/=10;
        }
        printf("%d",product);
        return 0;
    }
