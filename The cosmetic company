#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
    int n,r,s=9;
    scanf("%d",&n);
    while(n>0)
    {
        r=n%10;
        if(r<s){
            s=r;
        }
        n/=10;
    }
    printf("%d",s);
    /* Enter your code here. Read input from STDIN. Print output to STDOUT */    
    return 0;
}
