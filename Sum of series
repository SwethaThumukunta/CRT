#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
    int n,i;
    scanf("%d",&n);
    int sum=0;
    int term=0;
    for(i=1;i<=n;i++){
        term=term*10+2;
        sum=sum+term;
    }
    printf("%d",sum);
    

    /* Enter your code here. Read input from STDIN. Print output to STDOUT */    
    return 0;
}
