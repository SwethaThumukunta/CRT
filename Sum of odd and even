#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
    int n,sum_e=0,sum_o=0;
    scanf("%d",&n);
    int a[n],i;
    for(i=0;i<n;i++){
        scanf("%d",&a[i]);
    
    if(a[i]%2==0){
        sum_e=sum_e+a[i];
    }else{
        sum_o=sum_o+a[i];
    }
}
printf("%d\n%d",sum_o,sum_e);
    /* Enter your code here. Read input from STDIN. Print output to STDOUT */    
    return 0;
}
