#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
 float a,b,c;
    scanf("%f",&a);
    b=floor(a);
    c=ceil(a);
    printf("%d\n",(int)a);
    printf("%.1f\n",c);
    printf("%.1f",b);    
    return 0;
    /* Enter your code here. Read input from STDIN. Print output to STDOUT */    
    return 0;
}
